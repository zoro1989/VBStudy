/* pages/points/point-list.wxss */
page {
  background-color: #C7EDCC;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.point-detail-title{
  background-color: #333;
  padding: 10rpx;
  border-radius: 5rpx;
  color: #ffcd32;
}
.point-detail {
  height: 100%;
  display: flex;
  font-size: 28rpx;
  flex-direction: column;
  color: #333;
  font-weight: bolder;
  font-family: Arial, Helvetica, sans-serif
}